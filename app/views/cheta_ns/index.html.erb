<p id="notice"><%= notice %></p>

<h1>Listing Chetans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Commit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chetans.each do |chetan| %>
      <tr>
        <td><%= chetan.name %></td>
        <td><%= chetan.commit_id %></td>
        <td><%= link_to 'Show', chetan %></td>
        <td><%= link_to 'Edit', edit_chetan_path(chetan) %></td>
        <td><%= link_to 'Destroy', chetan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chetan', new_chetan_path %>
